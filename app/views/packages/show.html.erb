<h1>Packages#show</h1>
<p>Find me in app/views/packages/show.html.erb</p>


<table>
  <thead>
    <tr>
      <th>Country</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Company</th>
      <th>street</th>
      <th>street2</th>
      <th>city</th>
      <th>state</th>
      <th>zip</th>
      <th>phone</th>
      <th>email</th>
      <th colspan="12"></th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td><%= @package.country %></td>
      <td><%= @package.first_name %></td>
      <td><%= @package.last_name %></td>
      <td><%= @package.company %></td>
      <td><%= @package.street %></td>
      <td><%= @package.street2 %></td>
      <td><%= @package.city %></td>
      <td><%= @package.state %></td>
      <td><%= @package.zip %></td>
      <td><%= @package.phone %></td>
      <td><%= @package.email %></td>
    </tr>
  </tbody>
</table>
