<h1>Log In</h1>
<%= form_tag login_path do %>
  <p>
    <%= label_tag :username %>
    <%= text_field_tag :username %>
  </p>
  <p>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </p>
  <p>
    <%= submit_tag 'Log In' %>
  </p>

<% end %>
<p>
  <%= link_to 'Sign Up', new_user_path %>
</p>
